    <Style.Triggers>
      <Trigger Property="Validation.HasError" Value="True">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Margin" Value="0,0,0,20" />
      </Trigger>
      <Trigger Property="Validation.HasError" Value="False">
